<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="OmnisChain" location="startMenu"/>
	<shortcut
		name="OmnisChain Server"
		target="$INSTALL_PATH\OmnisChain.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start OmnisChain server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="OmnisChain Wallet"
		target="http://localhost:6969/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch OmnisChain wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>